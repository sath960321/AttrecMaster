<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attrecmaster">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_attrec"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_attrec"
        android:supportsRtl="true"
        android:theme="@style/Theme.AttrecMaster">
        <activity android:name=".AddAsistenciaActivity"></activity>
        <activity android:name=".AddEvaluacionActivity"></activity>
        <activity android:name=".ListadoEvaluacionActivity" />
        <activity android:name=".ListadoAsistenciasActivity" />
        <activity android:name=".AddEstudianteActivity" />
        <activity android:name=".ListadoEstudiantesActivity" />
        <activity android:name=".IntermedioActivity" />
        <activity android:name=".AddRegistroActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>