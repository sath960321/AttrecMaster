<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attrecmaster">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_attrec"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_attrec"
        android:supportsRtl="true"
        android:theme="@style/Theme.AttrecMaster">
        <activity android:name=".AddAsistenciaActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".AddEvaluacionActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ListadoEvaluacionActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ListadoAsistenciasActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".AddEstudianteActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ListadoEstudiantesActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".IntermedioActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".AddRegistroActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>